<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <description>发起支付请求</description>
    <display-name>发起支付请求</display-name>
    <servlet-name>PaymentRequest</servlet-name>
    <servlet-class>service.PaymentRequest</servlet-class>
  </servlet>
  <servlet>
    <description>Payment response</description>
    <display-name>Payment response</display-name>
    <servlet-name>PaymentResultResponse</servlet-name>
    <servlet-class>service.PaymentResultResponse</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>PaymentRequest</servlet-name>
    <url-pattern>/servlet/yeepay/paymentRequest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PaymentResultResponse</servlet-name>
    <url-pattern>/servlet/yeepay/PaymentResultResponse</url-pattern>
  </servlet-mapping>
  	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
